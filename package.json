{
  "name": "@poppinss/oauth-client",
  "version": "3.0.0",
  "description": "A package to implement \"Login with\" flow using OAuth compliant authorization servers.",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json poppinss/oauth-client"
  },
  "keywords": [
    "oauth",
    "oauth1",
    "oauth2"
  ],
  "author": "virk,poppinss",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^4.1.2",
    "@adonisjs/require-ts": "^2.0.8",
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.6",
    "cookie-parser": "^1.4.5",
    "del-cli": "^4.0.1",
    "doctoc": "^2.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.3",
    "eslint-plugin-prettier": "^3.4.1",
    "express": "^4.17.1",
    "github-label-sync": "^2.0.2",
    "husky": "^7.0.2",
    "japa": "^3.1.1",
    "mrm": "^3.0.8",
    "nock": "^13.1.3",
    "np": "^7.5.0",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "dependencies": {
    "@types/luxon": "^2.0.2",
    "got": "^11.8.2",
    "luxon": "^2.0.2"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poppinss/oauth-client.git"
  },
  "bugs": {
    "url": "https://github.com/poppinss/oauth-client/issues"
  },
  "homepage": "https://github.com/poppinss/oauth-client#readme"
}
