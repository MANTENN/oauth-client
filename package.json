{
  "name": "@poppinss/oauth-client",
  "version": "1.0.0",
  "description": "A package to implement \"Login with\" flow using OAuth compliant authorization servers.",
  "main": "index.js",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json null"
  },
  "keywords": [
    "oauth",
    "oauth1",
    "oauth2"
  ],
  "author": "virk,poppinss",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^3.0.0",
    "@adonisjs/require-ts": "^2.0.3",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "commitizen": "^4.2.3",
    "cookie-parser": "^1.4.5",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^3.0.1",
    "doctoc": "^2.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-adonis": "^1.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.1",
    "github-label-sync": "^2.0.0",
    "husky": "^6.0.0",
    "japa": "^3.1.1",
    "mrm": "^2.6.2",
    "nock": "^13.0.11",
    "np": "^7.4.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "dependencies": {
    "@types/luxon": "^1.26.3",
    "got": "^11.8.2",
    "luxon": "^1.26.0"
  }
}
